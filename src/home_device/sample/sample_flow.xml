<flow
	id="7"
	name="자동 세탁"
	description="월요일, 목요일 아침 자동 세탁 후 알림"
	isAuto="true">
	
	<trigger
		appid="SYS_TIMER"
		command="---"
		cond="ISEQUAL"
		value="10:00:16"/>
	
	<action
		appid=""
		command=""/>
	<loop
		appid="SYS_COUNTER"
		command="---">
		<action
			appid=""
			command=""/>
	</loop>
	<notification
		appid=""
		command=""/>
	<condition
		appid="SYS_TIMER"
		command="---"
		cond="LESS_OR_EQUAL"
		value="12:00:00">
		<action
			appid=""
			command=""/>
		<notification
			appid=""
			command=""/>
	</condition>
</flow>